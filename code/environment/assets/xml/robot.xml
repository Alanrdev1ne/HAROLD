            <body name="robot0:shoulder">
                    <geom mesh="robot0:shoulder_base" pos="-80 111 80"></geom>
                    <body name="robot0:shoulder_joint">
                        <geom mesh="robot0:shoulder_joint_1" material="robot0:base_mat" pos="0 230 0" quat=".5 .5 -.5 .5" class="robot0:blue"></geom>
                        <geom mesh="robot0:shoulder_joint_2" material="robot0:base_mat" pos="0 230 0" quat=".5 .5 .5 -.5" class="robot0:blue"></geom>
                        <joint name="robot0:shoulder_joint" pos="0 230 0" axis="1 0 0" type="hinge"></joint>
                    </body>
            </body>
      </body>
   </body>

</mujoco>
