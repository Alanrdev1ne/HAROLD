<?xml version="1.0" ?>

<robot name="Harold.urdf">
	<link name="base_link">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>	
			<mass value="100000000"/>
			<inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
		</inertial>

		<visual>
			<geometry>
				<mesh filename="stl/rotary_base.stl" scale="1 1 1"/>
			</geometry>
		</visual>

		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<mesh filename="stl/rotary_base.stl" scale="1 1 1"/>
			</geometry>
		</collision>
	</link>

	<joint name="base__rotator" type="revolute">
		<axis xyz="0 0 1"/>
		<limit effort="1000" lower="-6.28" upper="6.28" velocity="0.5"/>
		<origin rpy="0 0 0" xyz="0 0 6.5"/>
		<parent link="base_link"/>
		<child link="rotator_link"/>
	</joint>

	<link name="rotator_link">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 6.5"/>	
			<mass value="1"/>
			<inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
		</inertial>

		<visual>
			<geometry>
				<mesh filename="stl/rotator.stl" scale="1 1 1"/>
			</geometry>
		</visual>

		<collision>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="stl/rotator.stl" scale="1 1 1"/>
			</geometry>
		</collision>
	</link>

	<joint name="rotator__shoulder" type="revolute">
		<axis xyz="1 0 0"/>
		<limit effort="1000" lower="-2" upper="2" velocity="0.5"/>
		<origin rpy="0 0 0" xyz="0 0 16.41"/>
		<parent link="rotator_link"/>
		<child link="shoulder_link"/>
	</joint>

	<link name="shoulder_link">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 -6"/>	
			<mass value="1"/>
			<inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
		</inertial>

		<visual>
			<origin rpy="0 0 0" xyz="0 0 -6"/>
			<geometry>
				<mesh filename="stl/shoulder.stl" scale="1 1 1"/>
			</geometry>
		</visual>

		<collision>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="stl/shoulder.stl" scale="1 1 1"/>
			</geometry>
		</collision>
	</link>

	<joint name="shoulder__elbow" type="revolute">
		<axis xyz="1 0 0"/>
		<limit effort="1000" lower="-2" upper="2" velocity="0.5"/>
		<origin rpy="0 0 0" xyz="0 0 22"/>
		<parent link="shoulder_link"/>
		<child link="elbow_link"/>
	</joint>

	<link name="elbow_link">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 -4.27"/>
			<mass value="1"/>
			<inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
		</inertial>

		<visual>
			<origin rpy="0 0 0" xyz="0 0 -4.27"/>
			<geometry>
				<mesh filename="stl/elbow.stl" scale="1 1 1"/>
			</geometry>
		</visual>

		<collision>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="stl/elbow.stl" scale="1 1 1"/>
			</geometry>
		</collision>
	</link>

	<joint name="elbow__hand" type="revolute">
		<axis xyz="1 0 0"/>
		<limit effort="1000" lower="-2" upper="2" velocity="0.5"/>
		<origin rpy="0 0 0" xyz="0 0 22.28"/>
		<parent link="elbow_link"/>
		<child link="hand_link"/>
	</joint>

	<link name="hand_link">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 -3.4"/>
			<mass value="1"/>
			<inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
		</inertial>

		<visual>
			<origin rpy="0 0 0" xyz="0 0 -3.4"/>
			<geometry>
				<mesh filename="stl/hand.stl" scale="1 1 1"/>
			</geometry>
		</visual>

		<collision>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="stl/hand.stl" scale="1 1 1"/>
			</geometry>
		</collision>
	</link>

	<joint name="hand__servo_gear" type="revolute">
		<axis xyz="0 1 0"/>
		<limit effort="10" lower="-1.6" upper=".6" velocity="0.5"/>
		<origin rpy="0 0 0" xyz="1.3 0 6.5"/>
		<parent link="hand_link"/>
		<child link="servo_gear_link"/>
	</joint>

	<link name="servo_gear_link">
		<inertial>
			<origin rpy="0 0 0" xyz="-1.3 0 -1.25"/>
			<mass value="1"/>
			<inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
		</inertial>

		<visual>
			<origin rpy="0 0 0" xyz="-1.3 0 -1.25"/>
			<geometry>
				<mesh filename="stl/servo_gear.stl" scale="1 1 1"/>
			</geometry>
		</visual>

		<collision>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="stl/servo_gear.stl" scale="1 1 1"/>
			</geometry>
		</collision>
	</link>
	
	<joint name="servo_gear__finger_right" type="revolute">
		<axis xyz="0 1 0"/>
		<limit effort="1000" lower="-3.15" upper="3.15" velocity="0.5"/>
		<mimic joint="hand__servo_gear" multiplier="1" offset="0"/>
		<origin rpy="0 0 0" xyz="4 0 0"/>
		<parent link="servo_gear_link"/>
		<child link="finger_right_link"/>
	</joint>
	
	<link name="finger_right_link">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<mass value="1"/>
			<inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
		</inertial>

		<visual>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="stl/finger_right.stl" scale="1 1 1"/>
			</geometry>
		</visual>

		<collision>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="slt/finger_right.stl" scale="1 1 1"/>
			</geometry>
		</collision>
	</link>

	<joint name="hand__bar_right" type="revolute">
		<axis xyz="0 1 0"/>
		<limit effort="10" lower="-.6" upper="1.6" velocity="0.5"/>
		<mimic joint="hand__servo_gear" multiplier="-1"/>
		<origin rpy="0 0 0" xyz=".7 - 8.5"/>
		<parent link="hand_link"/>
		<child link="bar_right_link"/>
	</joint>

	<link name="bar_right_link">
		<inertial>
			<origin rpy="0 0 0" xyz="2 0 -.5"/>
			<mass value="1"/>
			<inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
		</inertial>

		<visual>
			<origin rpy="0 0 0" xyz="2 0 -.5"/>
			<geometry>
				<mesh filename="stl/bar.stl" scale="1 1 1"/>
			</geometry>
		</visual>
	</link>

	<joint name="hand__idle_gear" type="revolute">
		<axis xyz="0 1 0"/>
		<limit effort="10" lower="-1.6" upper=".6" velocity="0.5"/>
		<mimic joint="hand__servo_gear" multiplier="-1"/>
		<origin rpy="0 0 0" xyz="-1.3 0 6.5"/>
		<parent link="hand_link"/>
		<child link="idle_gear_link"/>
	</joint>

	<link name="idle_gear_link">
		<inertial>
			<origin rpy="0 0 0" xyz="1.3 0 -1.25"/>
			<mass value="1"/>
			<inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
		</inertial>

		<visual>
			<origin rpy="0 0 0" xyz="1.3 0 -1.25"/>
			<geometry>
				<mesh filename="stl/idle_gear.stl" scale="1 1 1"/>
			</geometry>
		</visual>
	</link>

	<joint name="hand__bar_left" type="revolute">
		<axis xyz="0 1 0"/>
		<limit effort="10" lower="-1.6" upper="0.6" velocity="0.5"/>
		<mimic joint="hand__servo_gear" multiplier="1"/>
		<origin rpy="0 0 0" xyz="-.7 0 8.5"/>
		<parent link="hand_link"/>
		<child link="bar_left_link"/>
	</joint>

	<link name="bar_left_link">
		<inertial>
			<origin rpy="0 0 0" xyz="-2 0 -.5"/>
			<mass value="1"/>
			<inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
		</inertial>

		<visual>
			<origin rpy="0 0 0" xyz="-2 0 -.5"/>
			<geometry>
				<mesh filename="stl/bar.stl" scale="1 1 1"/>
			</geometry>
		</visual>
	</link>

	<joint name="idle_gear__finger_left" type="revolute">
		<axis xyz="0 1 0"/>
		<limit effort="10" lower="-1.6" upper="0.6" velocity="0.5"/>
		<mimic joint="hand__servo_gear" multiplier="1"/>
		<origin rpy="0 0 0" xyz="-4 0 0"/>
		<parent link="idle_gear_link"/>
		<child link="finger_left_link"/>
	</joint>

	<link name="finger_left_link">
		<inertial>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<mass value="1"/>
			<inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100"/>
		</inertial>

		<visual>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="stl/finger_left.stl" scale="1 1 1"/>
			</geometry>
		</visual>
		
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry>
				<mesh filename="stl/finger_left.stl" scale="1 1 1"/>
			</geometry>
	</link>


</robot>
